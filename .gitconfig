[user]
	email = “emond.jesse@gmail.com”
	name = Jesse Emond
[push]
	default = simple
[core]
	autocrlf = false
	filemode = false
[color]
	ui = true
[alias]
	st   = status
	br   = branch
        ph   = push
        pl   = pull
        co   = checkout
        c    = commit
        cm   = commit -m 
        mg   = merge
        a    = add
        aa   = add --all
        tree = log --graph --decorate --pretty=oneline --abbrev-commit
        d    = diff
        dc   = diff --cached
	conflicts = diff --name-only --diff-filter=U
